  "id": "mBVi9C0eRoq1xTeb",
  "name": "[Service] Email Error Notification",
  "active": true,
  "isArchived": false,
  "nodes": [
    {
      "parameters": {
        "inputSource": "passthrough"
      },
      "id": "trigger",
      "name": "When Called by Another Workflow",
      "type": "n8n-nodes-base.executeWorkflowTrigger",
      "typeVersion": 1.1,
      "position": [
        0,
        0
      ]
    },
    {
      "parameters": {
        "jsCode": "const input = $json;\n\n// Validate required fields\nif (!input.error) {\n  throw new Error('error message is required');\n}\nif (!input.workflow_name) {\n  throw new Error('workflow_name is required');\n}\n\n// Format details as string if object\nlet detailsStr = input.details || '';\nif (typeof detailsStr === 'object') {\n  detailsStr = JSON.stringify(detailsStr, null, 2);\n}\n\nreturn [{\n  json: {\n    to_email: input.to_email || 'kelly@ad-pilot.ai',\n    workflow_name: input.workflow_name,\n    error: input.error,\n    details: detailsStr,\n    job_id: input.job_id || null\n  }\n}];"
      },
      "id": "format-input",
      "name": "Format Input",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        220,
        0
      ]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://api.resend.com/emails",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpBearerAuth",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"from\": \"notification@ad-pilot.ai\",\n  \"to\": \"{{ $json.to_email }}\",\n  \"subject\": \"{{ $json.workflow_name }} Failed{{ $json.job_id ? ' - Job ' + $json.job_id : '' }}\",\n  \"html\": {{ JSON.stringify('<h2>' + $json.workflow_name + ' Failed</h2><p><strong>Error:</strong> ' + $json.error + '</p>' + ($json.details ? '<h3>Details:</h3><pre>' + $json.details + '</pre>' : '')) }}\n}",
        "options": {}
      },
      "id": "send-email",
      "name": "Send Error Email",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        440,
        0
      ],
      "credentials": {
        "httpBearerAuth": {
          "id": "vdxOfyrKMfWKSmAH",
          "name": "Resend"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "const response = $json;\n\nreturn [{\n  json: {\n    success: !!response.id,\n    email_id: response.id || null,\n    error: response.error || null\n  }\n}];"
      },
      "id": "return-result",
      "name": "Return Result",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        660,
        0
      ]
    }
  ],
