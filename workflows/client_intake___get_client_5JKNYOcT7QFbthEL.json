      "id": "webhook-get",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "position": [
        272,
        304
      ],
      "webhookId": "client-get"
    },
    {
      "parameters": {
        "jsCode": "// Get the clientId from webhook query params (reference webhook node directly)\nconst clientId = $('Webhook').first().json.query.clientId;\n\n// Find the matching client from all rows\nconst allClients = $input.all();\nconst client = allClients.find(item => item.json.clientId === clientId);\n\nif (!client) {\n  return { json: { error: 'Client not found', clientId } };\n}\n\n// Parse JSON fields back to objects\nconst data = { ...client.json };\n\ntry {\n  if (data.platformStatuses) data.platformStatuses = JSON.parse(data.platformStatuses);\n} catch(e) { data.platformStatuses = {}; }\n\ntry {\n  if (data.additionalUsers) data.additionalUsers = JSON.parse(data.additionalUsers);\n} catch(e) { data.additionalUsers = []; }\n\ntry {\n  if (data.avatarPeople) data.avatarPeople = JSON.parse(data.avatarPeople);\n} catch(e) { data.avatarPeople = []; }\n\n// Convert platforms string back to array\nif (data.platforms && typeof data.platforms === 'string') {\n  data.platforms = data.platforms.split(', ').filter(p => p);\n}\n\n// Convert addons string back to array\nif (data.addons && typeof data.addons === 'string') {\n  data.addons = data.addons.split(', ').filter(a => a);\n}\n\n// Convert otherPhotosUrls string back to array\nif (data.otherPhotosUrls && typeof data.otherPhotosUrls === 'string') {\n  data.otherPhotosUrls = data.otherPhotosUrls.split(', ').filter(u => u);\n}\n\nreturn { json: { client: data } };"
      },
