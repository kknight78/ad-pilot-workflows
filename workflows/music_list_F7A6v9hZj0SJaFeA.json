  "name": "Music List",
  "active": true,
  "isArchived": false,
  "nodes": [
    {
      "parameters": {
        "httpMethod": "GET",
        "path": "music",
        "responseMode": "responseNode",
        "options": {
          "responseHeaders": {
            "entries": [
              {
                "name": "Access-Control-Allow-Origin",
                "value": "*"
              },
              {
                "name": "Access-Control-Allow-Methods",
                "value": "GET, OPTIONS"
              },
              {
                "name": "Access-Control-Allow-Headers",
                "value": "Content-Type"
              }
            ]
          },
          "allowedOrigins": "*"
        }
      },
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "position": [
        0,
        200
      ],
      "id": "webhook-music",
      "name": "Webhook",
      "webhookId": "music-list"
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT\n  s.id as song_id,\n  s.name,\n  s.artist,\n  s.audio_preview_url,\n  s.cloudinary_url,\n  s.filters\nFROM songs s\nJOIN client_music cm ON cm.song_id = s.id\nWHERE cm.client_id = '{{ $json.query.client_id }}' -- TODO: was hardcoded to CCC UUID, now requires ?client_id= query param\n  AND cm.active = true\nORDER BY s.name",
        "options": {}
      },
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.5,
      "position": [
        200,
        200
      ],
      "id": "query-music",
      "name": "Query Music",
      "credentials": {
        "postgres": {
          "id": "ZUwzqy3kvUJpXrDM",
          "name": "Postgres (Dev)"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "const tracks = $input.all().map(item => ({\n  song_id: item.json.song_id,\n  name: item.json.name,\n  artist: item.json.artist,\n  audio_url: item.json.audio_preview_url,\n  cloudinary_url: item.json.cloudinary_url,\n  filters: item.json.filters || [],\n  label: item.json.artist ? `${item.json.name} - ${item.json.artist}` : item.json.name\n}));\n\nreturn [{ json: { success: true, tracks } }];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        400,
        200
      ],
      "id": "format-response",
      "name": "Format Response"
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ $json }}",
        "options": {}
      },
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1.1,
      "position": [
        600,
        200
      ],
      "id": "respond",
      "name": "Respond"
    }
  ],
