  "name": "[Service] Remove Background",
  "active": true,
  "isArchived": false,
  "nodes": [
    {
      "parameters": {
        "inputSource": "passthrough"
      },
      "id": "trigger",
      "name": "When Called by Another Workflow",
      "type": "n8n-nodes-base.executeWorkflowTrigger",
      "typeVersion": 1.1,
      "position": [
        0,
        0
      ]
    },
    {
      "parameters": {
        "jsCode": "const input = $json;\n\n// Validate required fields\nif (!input.image_url) {\n  throw new Error('image_url is required');\n}\n\n// Set defaults for optional parameters\nreturn [{\n  json: {\n    image_url: input.image_url,\n    size: input.size || 'auto',\n    format: input.format || 'png',\n    crop: input.crop !== false,\n    crop_margin: input.crop_margin || '10%',\n    // Pass through any additional context\n    context: input.context || {}\n  }\n}];"
      },
      "id": "validate-input",
      "name": "Validate Input",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        220,
        0
      ]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://api.remove.bg/v1.0/removebg",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "x-api-key",
              "value": "72rkEtm8QRzFpqEmjL42o89z"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"image_url\": \"{{ $json.image_url }}\",\n  \"size\": \"{{ $json.size }}\",\n  \"format\": \"{{ $json.format }}\",\n  \"crop\": {{ $json.crop }},\n  \"crop_margin\": \"{{ $json.crop_margin }}\"\n}",
        "options": {
          "response": {
            "response": {
              "responseFormat": "file"
            }
          }
        }
      },
      "id": "remove-bg-api",
      "name": "API: Remove.bg",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        440,
        0
      ],
      "onError": "continueErrorOutput"
    },
    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "// Get input context from Validate Input node\nconst inputData = $('Validate Input').first().json;\n\n// Get the binary data\nconst binaryData = $input.item.binary?.data;\n\nif (!binaryData) {\n  throw new Error('No binary data returned from Remove.bg');\n}\n\n// Return success with binary and context\nreturn {\n  json: {\n    success: true,\n    format: inputData.format,\n    context: inputData.context\n  },\n  binary: {\n    data: binaryData\n  }\n};"
      },
      "id": "format-success",
      "name": "Format Success Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        660,
        -60
      ]
    },
    {
      "parameters": {
        "jsCode": "// Handle error from Remove.bg API\nconst error = $json;\n\nlet errorMessage = 'Unknown error';\n\n// Remove.bg error format: { \"errors\": [{ \"code\": \"...\", \"title\": \"...\" }] }\nif (error.errors && Array.isArray(error.errors) && error.errors.length > 0) {\n  const firstError = error.errors[0];\n  errorMessage = firstError.code \n    ? `${firstError.code}: ${firstError.title}` \n    : firstError.title;\n} else if (error.message) {\n  errorMessage = error.message;\n}\n\nreturn [{\n  json: {\n    success: false,\n    error: errorMessage,\n    raw_error: JSON.stringify(error, null, 2)\n  }\n}];"
      },
      "id": "format-error",
      "name": "Format Error Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        660,
        100
      ]
    }
  ],
