        "postgres": {
          "id": "3XsWrPuEddwVYbGS",
          "name": "Railway Postgres"
        }
      },
      "onError": "continueErrorOutput"
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT p.id as person_id, p.full_name, p.email, p.phone, p.notification_preference, a.style_name\nFROM avatars a\nJOIN persons p ON a.person_id = p.id\nWHERE a.motion_avatar_id = '{{ $(\"Extract Avatar Info\").first().json.avatar_id }}'",
        "options": {}
      },
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.5,
      "position": [
        1344,
        -48
      ],
      "id": "get-person-info",
      "name": "DB: Get Person Info",
      "credentials": {
        "postgres": {
          "id": "3XsWrPuEddwVYbGS",
          "name": "Railway Postgres"
        }
      },
      "onError": "continueErrorOutput"
    },
    {
      "parameters": {
        "jsCode": "const person = $json;\nconst avatarInfo = $(\"Extract Avatar Info\").first().json;\n\nreturn [{\n  json: {\n    person_id: person.person_id,\n    email: person.email,\n    phone: person.phone,\n    notification_pref: person.notification_preference || 'email',\n    subject: 'Your Avatar is Ready for Review',\n    message: 'Great news! Your ' + (person.style_name || 'new') + ' avatar motion has been generated and is ready for your review.'\n  }\n}];"
      },
      "type": "n8n-nodes-base.code",
