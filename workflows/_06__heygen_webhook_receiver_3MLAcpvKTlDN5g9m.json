    {
      "parameters": {
        "operation": "executeQuery",
        "query": "UPDATE avatars\nSET preview_video_url = '{{ $(\"Extract Avatar Info\").first().json.motion_preview_url }}',\n    preview_image_url = '{{ $(\"Extract Avatar Info\").first().json.image_url }}',\n    motion_status = 'complete',\n    approval_status = 'pending_review',\n    updated_at = NOW()\nWHERE motion_avatar_id = '{{ $(\"Extract Avatar Info\").first().json.avatar_id }}';",
        "options": {}
      },
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.5,
