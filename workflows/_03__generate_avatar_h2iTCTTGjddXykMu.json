    {
      "parameters": {
        "jsCode": "const transcription = $json;\nconst config = $('Parse Avatar Response').first().json;\nconst script = config.script;\nconst templateId = config.template_id || '';\n\n// Get Whisper word timestamps\nconst words = transcription.words.map(w => ({\n  text: w.word.replace(/[.,!?;:\\-\"'()]/g, '').toLowerCase(),\n  originalText: w.word,\n  start: w.start,\n  end: w.end\n}));\n\nconsole.log(`üìä Total Whisper words: ${words.length}`);\nconsole.log(`üìã Template: ${templateId}`);\n\n// Helper to normalize text for comparison\nconst normalizeWord = (word) => word.replace(/[.,!?;:\\-\"'()]/g, '').toLowerCase();\n\n// Build section texts based on template type\nlet sections = [];\n\nif (templateId === 'ad-single-item-spotlight-1') {\n  // SPOTLIGHT template: hero is already on screen during hook\n  const combinedHookText = script.hook + ' ' + (script.segments[0] || '');\n  sections = [\n    { name: 'hook', text: combinedHookText },\n    ...script.segments.slice(1).map((text, i) => ({ name: `segment_${i + 1}`, text })),\n    { name: 'cta', text: script.cta }\n  ];\n} else {\n  // Default: standard section building\n  sections = [\n    { name: 'hook', text: script.hook },\n    ...script.segments.map((text, i) => ({ name: `segment_${i}`, text })),\n    { name: 'cta', text: script.cta }\n  ];\n}\n\nconsole.log(`üìù Sections to process: ${sections.map(s => s.name).join(', ')}`);\n\nconst sectionTimings = [];\nlet wordIndex = 0;\n\nfor (const section of sections) {\n  const sectionWords = section.text\n    .split(/\\s+/)\n    .filter(w => w.length > 0)\n    .map(w => normalizeWord(w));\n\n  const wordCount = sectionWords.length;\n\n  console.log(`\\nüìç Section \"${section.name}\": ${wordCount} words`);\n\n  const startWord = words[wordIndex];\n  if (!startWord) {\n    console.warn(`‚ö†Ô∏è No start word for section ${section.name}`);\n    continue;\n  }\n\n  const expectedEndIndex = wordIndex + wordCount - 1;\n  const endWord = words[expectedEndIndex];\n\n  if (!endWord) {\n    console.warn(`‚ö†Ô∏è No end word for section ${section.name}`);\n    const lastWord = words[words.length - 1];\n    if (lastWord) {\n      sectionTimings.push({\n        name: section.name,\n        start: startWord.start,\n        end: lastWord.end,\n        duration: lastWord.end - startWord.start\n      });\n    }\n    break;\n  }\n\n  // Verify match and search for better alignment if needed\n  const lastSectionWord = sectionWords[wordCount - 1];\n  const timestampWord = normalizeWord(endWord.originalText);\n\n  if (lastSectionWord !== timestampWord) {\n    console.log(`   ‚ö†Ô∏è Word mismatch: script=\"${lastSectionWord}\" vs whisper=\"${timestampWord}\"`);\n    \n    let foundIndex = -1;\n    for (let offset = -3; offset <= 3; offset++) {\n      const checkIndex = expectedEndIndex + offset;\n      if (checkIndex >= 0 && checkIndex < words.length) {\n        if (normalizeWord(words[checkIndex].originalText) === lastSectionWord) {\n          foundIndex = checkIndex;\n          console.log(`   ‚úì Found match at offset ${offset}`);\n          break;\n        }\n      }\n    }\n\n    if (foundIndex !== -1) {\n      const correctedEndWord = words[foundIndex];\n      sectionTimings.push({\n        name: section.name,\n        start: startWord.start,\n        end: correctedEndWord.end,\n        duration: correctedEndWord.end - startWord.start\n      });\n      wordIndex = foundIndex + 1;\n      console.log(`   ‚úì Section timing: ${startWord.start}s - ${correctedEndWord.end}s`);\n      continue;\n    }\n  }\n\n  sectionTimings.push({\n    name: section.name,\n    start: startWord.start,\n    end: endWord.end,\n    duration: endWord.end - startWord.start\n  });\n\n  console.log(`   ‚úì Section timing: ${startWord.start}s - ${endWord.end}s`);\n  wordIndex += wordCount;\n}\n\nconsole.log(`\\nüìä Summary:`);\nconsole.log(`   Section timings: ${sectionTimings.map(s => s.name).join(', ')}`);\n\nreturn [{\n  json: {\n    ...config,\n    _section_timings: sectionTimings,\n    whisper_transcription: {\n      text: transcription.text,\n      duration: transcription.duration,\n      language: transcription.language\n    }\n  }\n}];"
      },
